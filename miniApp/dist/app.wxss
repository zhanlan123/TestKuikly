/** 隐藏滚动条 **/
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}

@font-face {
  font-family: 'qui-icons-1';
  src: url('https://static.gamecenter.qq.com/qgame/fonts/qui-icons-1.ttf') format('truetype');
}

[class^="icon-"]:before, [class*=" icon-"]:before {
  font-family: "qui-icons-1";
  text-align: center;
}

/* 转菊花的动画样式 */
@keyframes activityIndicatorRotate {
    0% { transform: rotate(0deg) }
    100% { transform: rotate(360deg) }
